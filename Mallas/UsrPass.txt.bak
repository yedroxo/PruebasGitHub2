declare
v_nombtabla varchar2(30);
cv_cur1 SYS_REFCURSOR;
v_cadena varchar2(200);
begin
    
    DBMS_OUTPUT.disable;
    DBMS_OUTPUT.ENABLE(500000);
    open cv_cur1 for 
    select TABLE_NAME
    from ALL_TABLES  
    where owner = 'GORAPR'
    and TABLE_NAME like 'TLP%'
    order by TABLE_NAME;
    
     loop
                 
         FETCH cv_cur1 INTO v_nombtabla;
         EXIT WHEN cv_cur1%NOTFOUND;
                         
         v_cadena := 'select /*+ parallel(tabla, default) */  ''' || v_nombtabla ||''' nomTabla, count(*) conteo from gorapr.' || v_nombtabla || ' tabla union';
         DBMS_OUTPUT.PUT_LINE(v_cadena);                                         
    end loop;
    close cv_cur1; 
END;



