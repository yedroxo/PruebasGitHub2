declare 
  TYPE rowid_array is table of ROWID;
  TYPE string_array is table of varchar2(40);
  m_rowid rowid_array;
  v_cliente string_array;
  v_retencion string_array;
  v_sttarjeta string_array;
  v_chip string_array;
  v_particion varchar2(40);
  v_sql varchar2(700);
  v_tabla varchar2(20);
  cv_cur1 SYS_REFCURSOR;
  cv_cur2 SYS_REFCURSOR;
  v_limite integer := 0;
BEGIN
          v_limite :=100000;
          v_tabla := 'TFR065_LOGAUTOCORT';  --TFR065_LOGAUTOCORT TFR055_LOG_AUTORIZADOR
         
          open cv_cur1 for
            select subobject_name
            from sys.ALL_OBJECTS
            where owner = 'GORAPR'
            and object_name = v_tabla
            and subobject_name is not null
            order by subobject_name;
            
            loop
                                      
                 FETCH cv_cur1 INTO v_particion;
                 EXIT WHEN cv_cur1%NOTFOUND;
                         
